{
  "manifest_version": 2,

  "name": "reElevate",
  "version": "0.2.0",
  "description": "Elevate Radiopaedia playlists",
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "css": ["styles.css"],
    "js": [
      "vendor/jquery-3.3.1.slim.min.js",
      "vendor/jquery.hotkeys-0.2.0.js",
      "elevate.js",
      "keys.js"
    ],
    "matches": [
      "https://radiopaedia.org/play/*",
      "file://*pages/*"
    ]
  }]
}